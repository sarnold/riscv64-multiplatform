Subject: [PATCH 81/86] We cannot select PM when selecting bcmdhd module, which
 will make usb3.0 hub faild

---
 drivers/net/wireless/broadcom/bcmdhd/Kconfig | 2 --
 1 file changed, 2 deletions(-)

diff --git a/drivers/net/wireless/broadcom/bcmdhd/Kconfig b/drivers/net/wireless/broadcom/bcmdhd/Kconfig
index 114a50323502..fffcee2004d8 100644
--- a/drivers/net/wireless/broadcom/bcmdhd/Kconfig
+++ b/drivers/net/wireless/broadcom/bcmdhd/Kconfig
@@ -1,7 +1,5 @@
 config BCMDHD
 	tristate "Broadcom FullMAC wireless cards support"
-    select HIBERNATE_CALLBACKS
-    select PM_SLEEP 
 	help
 	  This module adds support for wireless adapters based on
 	  Broadcom FullMAC chipset.
